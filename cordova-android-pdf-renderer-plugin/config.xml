<?xml version='1.0' encoding='utf-8'?>
<widget id="com.bjss.pdf" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CordovaPDFRendererPlugin</name>
    <description>
        A sample Apache Cordova application that demonstrates the Cordova PDF Renderer Plugin
    </description>
    <author email="john.riley@bjss.com" href="https://github.com/johnrileybjss">
        John Riley - BJSS
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <engine name="android" spec="^6.2.3" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
